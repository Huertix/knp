    <script>
        Core.courseTitle = 'Joyful Development with Symfony 3';
        Core.courseId = 74
        Core.courseIsOwned = true;
        Core.courseSlug = 'symfony';
                    Core.chapterTitle = 'JSON Responses + Route Generation';
