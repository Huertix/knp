    <script>
        Core.courseTitle = 'OOP (course 4): Static methods, Namespaces, Exceptions &amp; Traits! Woh!';
        Core.courseId = 82
        Core.courseIsOwned = false;
        Core.courseSlug = 'oo-ep4';
