    <script type="text/javascript" src="https://js.stripe.com/v1/"></script>
<script type="text/javascript" src="../../assets/vendor/bootstrap-select/dist/js/bootstrap-select.js"></script>
<script src="../../assets/vendor/jquery.payment/jquery.payment.min.js"></script>
<script type="text/javascript" src="../../bundles/knpuniversity/js/StripePayment.js?v=3"></script>

<script type="text/javascript">
    Stripe.setPublishableKey('pk_HFsr7dxtOllyxO5t2HksczTrlZSXd');
