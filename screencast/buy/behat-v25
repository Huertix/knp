    <script src="https://js.braintreegateway.com/js/braintree-2.26.0.min.js"></script>
    <script src="../../builds/checkout_login_registration-1dfa89b76a105f9c2d98.js"></script>

    <script type="text/javascript">
        // is the registration form or login form active?
        var isRegistrationFormActive = true;
        var globalCurrentSubscriptionLength = ''

        var paypalCheckout;
        function openPayPalCheckoutWindow() {
            // the paypalCheckout is set after Braintree sets itself up
            // but in tests, things happen so quickly, this may not have
            // finished yet

            if (!paypalCheckout) {
                setTimeout(openPayPalCheckoutWindow, 500);

                return;
            }

            paypalCheckout.paypal.initAuthFlow();
        }

        function showPaypalPaymentErrors(error) {
            jQuery('.js-braintree-payment-errors')
                .show()
                .html(error.message);
        }
        function hidePaypalPaymentErrors() {
            jQuery('.js-braintree-payment-errors')
                .hide()
                .html('');
        }
        function enablePaypalSubmitButton() {
            jQuery('.js-checkout-paypal-submit-button')
                .removeAttr('disabled')
                .html('Checkout with <i class="fa fa-paypal"></i>')
                .loadingAnimation('hide');
        }
        function disablePaypalSubmitButton() {
            jQuery('.js-checkout-paypal-submit-button')
                .attr('disabled', 'disabled')
                .text('Processing...')
                .loadingAnimation();
        }
        jQuery(document).ready(function() {
            var paypalSubmitButton = $('.js-checkout-paypal-submit-button');
                            // @TODO Refactor all Braintree logic here with a separate class
                var clientToken = '';
                braintree.setup(clientToken, 'paypal', {
                    container: $('.js-paypal-payment-container'),
                    displayName: 'KnpUniversity',
                    singleUse: false,
                    dataCollector: {
                        paypal: true  // Enables fraud prevention
                    },
                    // Doesn't display PayPal button (handle with '.js-checkout-paypal-submit-button' custom button)
                    // More info on the https://developers.braintreepayments.com/reference/client-reference/javascript/v2/best-practices#custom-ui
                    headless: true,
                    onReady: function (integration) {
                        paypalCheckout = integration;
                    },
                    // A callback function that is fired when the customer cancels
                    // or closes out of the PayPal flow before successful completion.
                    onAuthorizationDismissed: function () {
                        enablePaypalSubmitButton();
                    },
                    onPaymentMethodReceived: function (result) {
                        disablePaypalSubmitButton();

                        // Serialize company details form data to pass to the server
                        var serializedFormData = paypalSubmitButton.closest('form.js-braintree-payment-form').serialize();

                        if (Core.isUserAuthenticated()) {
                            // sends the checkout request and handles what to do next
                            CheckoutProcessor.sendCheckoutRequest(
                                'braintree',
                                {
                                    payment_method_nonce: result.nonce
                                },
                                serializedFormData
                            );
                        } else {
                            submitLoginOrRegForm(function(isNowAuthed) {
                                if (isNowAuthed) {
                                    // sends the checkout request and handles what to do next
                                    CheckoutProcessor.sendCheckoutRequest(
                                        'braintree',
                                        {
                                            payment_method_nonce: result.nonce
                                        },
                                        serializedFormData
                                    );
                                } else {
                                    // scroll up to where the error message should be
                                    scrollToAuthenticationBox();
                                    enablePaypalSubmitButton();
                                }
                            });
                        }
                    }
                });

                paypalSubmitButton.on('click', function (event) {
                    event.preventDefault();

                    disablePaypalSubmitButton();
                    hidePaypalPaymentErrors();

                    if (Core.isUserAuthenticated()) {
                        openPayPalCheckoutWindow();
                    } else {
                        submitLoginOrRegForm(function(isNowAuthed) {
                            if (isNowAuthed) {
                                openPayPalCheckoutWindow();
                            } else {
                                // scroll up to where the error message should be
                                scrollToAuthenticationBox();
                                enablePaypalSubmitButton();
                            }
                        });
                    }
                });
            
             $('.js-payment-switcher').on('change', function () {
                 var type = $(this).data('type');
                 var isChecked = $(this).is(':checked');
                 if (type == 'creditcard') {
                     $('.js-creditcard').show();
                     $('.js-paypal').hide();
                 } else {
                     $('.js-creditcard').hide();
                     $('.js-paypal').show();
                 }
             });

            StripePayment.initialize($('#payment-form'), true);

            // toggle between login and registration forms
            $('.js-toggle-auth').on('click', function(e) {
                e.preventDefault();

                var toggle = $(this).data('toggle');
                if (!toggle) {
                    console.log('No toggle on link!');
                }

                var $login = $('.js-toggle-auth-login');
                var $loginHeader = $('.js-toggle-auth-header-login');
                var $register = $('.js-toggle-auth-register');
                var $registerHeader = $('.js-toggle-auth-header-register');

                if (toggle == 'login') {
                    isRegistrationFormActive = false;
                    $register.addClass('hidden');
                    $registerHeader.addClass('hidden');

                    $login.removeClass('hidden');
                    $loginHeader.removeClass('hidden');
                } else {
                    isRegistrationFormActive = true;
                    $login.addClass('hidden');
                    $loginHeader.addClass('hidden');

                    $register.removeClass('hidden');
                    $registerHeader.removeClass('hidden');
                }

                $('.js-toggle-auth').removeClass('hidden');
                $(this).addClass('hidden');
            });

            
            // Change link icon from plus to minus for opened TB collapse and vice versa
            $('.collapse')
                    .on('show.bs.collapse', function () {
                        var linkSelector = 'a[href="#' + $(this).attr('id') + '"]';
                        $(linkSelector)
                                .find('.fa')
                                .removeClass('fa-plus')
                                .addClass('fa-minus');
                    })
                    .on('hide.bs.collapse', function () {
                        var linkSelector = 'a[href="#' + $(this).attr('id') + '"]';
                        $(linkSelector)
                                .find('.fa')
                                .removeClass('fa-minus')
                                .addClass('fa-plus');
                    });
        });

        // initialize the login form js
        var $authSuccessBox = $('.js-authentication-success-box');
        var $loginRegisterWrapper = $('.js-login-register-wrapper');
        var loginForm = new LoginForm($loginRegisterWrapper, $authSuccessBox);
        var registerForm = new RegisterForm($loginRegisterWrapper, $authSuccessBox);

        jQuery('.js-prevent-double-submit').on('submit', function(e) {
            var $button = $(this).find('.js-submit-button');

            if ($button.attr('disabled') == 'disabled') {
                // prevent double-submit
                e.preventDefault();
            } else {
                // mark it as submitted
                $button.attr("disabled", "disabled");
            }
        });

        // simple coupon JS
        jQuery('.js-show-coupon-code').on('click', 'a', function(e) {
            e.preventDefault();

            $(e.delegateTarget).hide();
            $('.js-coupon-form').removeClass('hidden');
        });

        jQuery('.js-coupon-form').on('submit', function(e) {
            if (Core.isUserAuthenticated()) {
                // allow the normal submit!
                return;
            }

            e.preventDefault();

            var $couponForm = $(this);
            var $submitButton = $couponForm.find('button');
            $submitButton.loadingAnimation();
            submitLoginOrRegForm(function(isNowAuthed) {
                $submitButton.loadingAnimation('hide');

                if (isNowAuthed) {
                    $couponForm.submit();
                } else {
                    var $loginErrorBox = $('.js-login-before-coupon');
                    $loginErrorBox.removeClass('hidden');

                    var $registrationWord = $loginErrorBox.find('.js-login-message-registration');
                    var $loginWord = $loginErrorBox.find('.js-login-message-login');
                    if (isRegistrationFormActive) {
                        $registrationWord.show();
                        $loginWord.hide();
                    } else {
                        $registrationWord.hide();
                        $loginWord.show();
                    }

                    scrollToAuthenticationBox();
                }
            });
        });

        var scrollToAuthenticationBox = function() {
            $('.js-login-register-wrapper').smoothScroll();
            $.smoothScroll({
                scrollTarget: $loginRegisterWrapper
            });
        };

        var submitLoginOrRegForm = function(callback) {
            if (isRegistrationFormActive) {
                registerForm.submitForm().done(function(data) {
                    callback(data.registered);
                });
            } else {
                loginForm.submitForm().done(function(data) {
                    callback(data.authenticated);
                });
            }
        };

        var checkoutApp = jQuery('.js-entire-checkout-app');

        checkoutApp.on('click', '.js-checkout-submit-button', function(e) {
            // authenticated? Let the form submit as normal!
            if (Core.isUserAuthenticated()) {
                return;
            }

            e.preventDefault();
            StripePayment._disableSubmitButton();

            submitLoginOrRegForm(function(isNowAuthed) {
                if (isNowAuthed) {
                    // Send payment form if user successfully registered
                    StripePayment.$formEl.submit();
                } else {
                    // scroll up to where the error message should be
                    scrollToAuthenticationBox();
                    StripePayment._enableSubmitButton();
                }
            });
        });

        // free checkout handling
        checkoutApp.on('click', '.js-checkout-for-free-button', function(e) {
            e.preventDefault();

            $(this)
                .attr('disabled', 'disabled')
                .loadingAnimation();

            CheckoutProcessor.sendCheckoutRequest(
                'free',
                {}
            );
        });

        // clicking between monthly and yearly
        jQuery('.js-change-subscription-length').on('click', function(e) {
            e.preventDefault();

            var $a = $(this);
            var $activeTarget = $a.find('.js-subscription-active-target');

            // 0) start a loading icon
            $activeTarget.loadingAnimation();

            // 5) update subscription length JS for team
            // team size change URL needs to know what subscription length to use
            globalCurrentSubscriptionLength = $(this).data('length');

            // 1) make an AJAX to change the order
            $.ajax({
                method: 'POST',
                url: $(this).attr('href')
            }).done(function(data) {
                $activeTarget.loadingAnimation('hide');

                // 2) change active states on subscription boxes
                // remove from everywhere
                $('.js-subscription-active-target').removeClass('active');
                // put back on MY child
                $activeTarget.addClass('active');

                // 3) update the total number
                $('.js-total-amount').html(data.total);

                // 4) if free-no-free change, reload the page
                var previouslyFree = data.previous_total == 0;
                var nowFree = data.total == 0;
                if (previouslyFree != nowFree) {
                    // the page changes a lot between free and non-free. Reload!
                    window.location.reload();
                }
            });
        });
    </script>
