    <script>
        Core.courseTitle = 'Symfony RESTful API: Basics++ (Course 3)';
        Core.courseId = 61
        Core.courseIsOwned = false;
        Core.courseSlug = 'symfony-rest3';
                    Core.chapterTitle = 'Pagination Links';
