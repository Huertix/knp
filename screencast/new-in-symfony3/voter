        Core.courseTitle = 'The new Awesome of Symfony 3.0';
        Core.courseId = 72
        Core.courseIsOwned = false;
        Core.courseSlug = 'new-in-symfony3';
                    Core.chapterTitle = 'The new Voter Class';
