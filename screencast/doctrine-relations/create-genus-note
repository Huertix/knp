        Core.courseTitle = 'Mastering Doctrine Relationships in Symfony';
        Core.courseId = 81
        Core.courseIsOwned = false;
        Core.courseSlug = 'doctrine-relations';
                    Core.chapterTitle = 'Create Genus Note';
