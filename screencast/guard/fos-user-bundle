    <script>
        Core.courseTitle = 'KnpUGuard: Symfony Authentication with a Smile';
        Core.courseId = 65
        Core.courseIsOwned = true;
        Core.courseSlug = 'guard';
                    Core.chapterTitle = 'FOSUserBundle';
