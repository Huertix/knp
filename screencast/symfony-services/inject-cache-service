    <script>
        Core.courseTitle = 'Symfony: Level up with Services and the Container';
        Core.courseId = 76
        Core.courseIsOwned = false;
        Core.courseSlug = 'symfony-services';
                    Core.chapterTitle = 'Injecting the Cache Service';
