    <script>
        Core.courseTitle = 'Symfony: Doctrine &amp; the Database';
        Core.courseId = 75
        Core.courseIsOwned = false;
        Core.courseSlug = 'symfony-doctrine';
                    Core.chapterTitle = 'Delightful Dummy Data with Alice';
