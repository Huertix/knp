    <script>
        Core.courseTitle = 'Stripe: Killer Payment Processing + ??? = Profit';
        Core.courseId = 84
        Core.courseIsOwned = false;
        Core.courseSlug = 'stripe';
                    Core.chapterTitle = 'Building the Custom Checkout Form';
