    <script>
        Core.courseTitle = 'Symfony Forms: Build, Render &amp; Conquer!';
        Core.courseId = 77
        Core.courseIsOwned = false;
        Core.courseSlug = 'symfony-forms';
                    Core.chapterTitle = 'The Form Type Class';
