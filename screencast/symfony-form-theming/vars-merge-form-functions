    <script>
        Core.courseTitle = 'Getting Crazy with Form Themes';
        Core.courseId = 83
        Core.courseIsOwned = false;
        Core.courseSlug = 'symfony-form-theming';
                    Core.chapterTitle = 'Go Deeper: Vars, Twig merge &amp; Form Functions';
            Core.chapterNumber = 8;
