    <script>
        Core.courseTitle = 'Symfony RESTful API: Hypermedia, Links &amp; Bonuses (Course 5)';
        Core.courseId = 63
        Core.courseIsOwned = false;
        Core.courseSlug = 'symfony-rest5';
