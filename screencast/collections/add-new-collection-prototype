    var disqus_shortname = 'knpuniversity';
    var disqus_container_id = 'disqus_thread';var disqus_developer = 1;var disqus_identifier = 'course_video_add-new-collection-prototype';var disqus_url = 'https://knpuniversity.com/screencast/collections/add-new-collection-prototype';var disqus_config = function () {};

