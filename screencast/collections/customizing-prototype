            <td class="blob-code"></span><span class="hljs-template-variable">{{ form_start(genusForm) }}</span><span class="xml"></td>
            </tr>
    <tr>
                    <td class="line-number line-number-expandable" data-range="15-40" data-last="0">
                <i class="fa fa-arrows-v js-icon-load-more" data-toggle="tooltip" title="Show Lines" data-placement="left"></i>
                <i class="fa fa-spinner fa-spin js-icon-loading"></i>
