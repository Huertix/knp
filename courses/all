    <script src="../assets/vendor/isotope/dist/isotope.pkgd.min.js"></script>
    <script>
        var courseHolder = $('.js-isotope-row').isotope({
            itemSelector: '.js-isotope-col'
        });
        var courseCollection = courseHolder.find('>.js-isotope-col');
        var filterTags = $('.js-filter-tag');

        filterTags.on('change', function () {
            // Get all checked tags
            var checkedTags = filterTags.filter(':checked');
            // Get all unchecked tags
            var uncheckedTags = filterTags.filter(':not(:checked)');

            courseHolder.isotope({
                filter: function () {
                    // Show all courses if no tag was checked
                    if (0 == checkedTags.length) {
                        return true;
                    }

                    var hasAllCheckedTags = true;
                    // Get course tags string and explode it to array
                    var courseTags = $(this).data('tags').split(', ');
                    // Find checked tags in course tags
                    checkedTags.each(function () {
                        var checkedTag = $(this).val();
                        // Exclude this course from list and stop iterating
                        // if at least one tag is missing
                        if (-1 === $.inArray(checkedTag, courseTags)) {
                            hasAllCheckedTags = false;

                            return false;
                        }
                    });

                    return hasAllCheckedTags;
                }
            });

            var countFilteredCourses = courseHolder.isotope('getFilteredItemElements').length;
            if (countFilteredCourses) {
                hideEmptyMessage();
            } else {
                showEmptyMessage();
            }

            // Add/remove "checked" class for tag's labels based on checkbox attribure
            filterTags.each(function(){
                var currentTagElement = $(this);
                var currentTagLabelElement = currentTagElement.next('label');
                if (currentTagElement.filter(':checked').length) {
                    currentTagLabelElement.addClass('checked');
                } else {
                    currentTagLabelElement.removeClass('checked');
                }
            });

            // process all unchecked tags
            uncheckedTags.each(function(){
                var currentTagElement = $(this);
                var tags = [];
                checkedTags.each(function(){
                    tags.push($(this).val());
                });
                tags.push(currentTagElement.val());

                var countCourses = 0;
                // Count courses which contains all already checked tags AND currentCourseTag
                courseCollection.each(function(){
                    if (isCourseHasAllTags($(this), tags)) {
                        countCourses++;
                    }
                });

                updateCourseNumberForTag(currentTagElement, countCourses);

                if (countCourses == 0) {
                    // prevent them from being clicked
                    $(this).prop('disabled', true);
                } else {
                    $(this).prop('disabled', false);
                }
            });

            // process all checked tags
            checkedTags.each(function(){
                var currentTagElement = $(this);
                var tags = [];
                checkedTags.each(function(){
                    if (currentTagElement.val() != $(this).val()) {
                        tags.push($(this).val());
                    }
                });

                // for checked tags, we always show how many are *actually* showing
                updateCourseNumberForTag(currentTagElement, countFilteredCourses);
            });
        });

        var emptyMessageElement = $('.js-course-filter-empty-message');
        function showEmptyMessage () {
            emptyMessageElement.fadeIn();
        }

        function hideEmptyMessage () {
            emptyMessageElement.fadeOut();
        }

        /**
         * Update number of related courses for given tag and disable its label if there're no courses
         *
         * @param {object} tagElement jQuery element of tag checkbox
         * @param {int} numberOfCourses
         */
        function updateCourseNumberForTag(tagElement, numberOfCourses) {
            var tagLabelElement = tagElement.next('label');
            if (numberOfCourses) {
                tagLabelElement.removeClass('disabled');
            } else {
                tagLabelElement.addClass('disabled');
            }


            var badgeText = '0';
            if (numberOfCourses > 0) {
                badgeText = numberOfCourses;
            }
            tagLabelElement.find('.badge').text(badgeText);
        }

        /**
         * Check whether given course has all given tags
         *
         * @param {object} courseElement jQuery element of course tag
         * @param {array} tags
         *
         * @returns {boolean}
         */
        function isCourseHasAllTags (courseElement, tags) {
            var hasAllTags = true;

            // Get course tags string and explode it to array
            var courseTags = courseElement.data('tags').split(', ');
            // Find checked tags in course tags
            for (var index in tags) {
                // Exclude this course from list and stop iterating
                // if at least one tag is missing
                if (-1 === $.inArray(tags[index], courseTags)) {
                    hasAllTags = false;

                    return false;
                }
            }

            return hasAllTags;
        }
    </script>
